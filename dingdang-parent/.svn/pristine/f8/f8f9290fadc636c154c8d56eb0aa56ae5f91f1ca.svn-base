package com.dingdang.biz.entity;

import java.io.Serializable;


/**
* @author 作者 Name:CaoGang
* @version 创建时间：2017年9月15日 上午9:52:29
* 类说明:用于生成验证码
*/
public class VerificationCode implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 8140289426673670734L;
	/**标签:发送验证码标签定义*/
	private String SignName;
	/**手机号码*/
	private String PhoneNumbers;
	/**短信模板*/
	private String TemplateCode;
	/**短信内容  格式:{\"name\":\"TemplateName\", \"code\":\"TemplateCode\"}*/
	private String TemplateParam;
	/**记录验证码*/
	private String Code;
	public String getSignName() {
		return SignName;
	}
	public void setSignName(String signName) {
		SignName = signName;
	}
	public String getPhoneNumbers() {
		return PhoneNumbers;
	}
	public void setPhoneNumbers(String phoneNumbers) {
		PhoneNumbers = phoneNumbers;
	}
	public String getTemplateCode() {
		return TemplateCode;
	}
	public void setTemplateCode(String templateCode) {
		TemplateCode = templateCode;
	}
	public String getTemplateParam() {
		return TemplateParam;
	}
	public void setTemplateParam(String templateParam) {
		TemplateParam = templateParam;
	}
	public String getCode() {
		return Code;
	}
	public void setCode(String code) {
		Code = code;
	}
	@Override
	public String toString() {
		return "VerificationCode [SignName=" + SignName + ", PhoneNumbers=" + PhoneNumbers + ", TemplateCode="
				+ TemplateCode + ", TemplateParam=" + TemplateParam + ", Code=" + Code + "]";
	}
	
}
