	<select id="selectFind" resultType="map">
		SELECT
		a.id as ID,
		a.u_name as name,
		CASE a.sex WHEN 0 THEN '男'
		WHEN 1
		THEN '女'
		ELSE '未设置' END as sex,
		a.u_phone as phone,
		a.account,
		a.dealers_id as dealersId,
		a.dealers_name as dealersName,
		ms.adress,
		a.type,
		si.u_phone as housekeeper,
		a.flag
		FROM a_user a LEFT JOIN
		merchant_site ms ON a.id =ms.auser_id
		and ms.def_checked=2
		LEFT JOIN staff_info si on
		a.staff_id=si.id
		<where>
			<if test="find!=null and find!=''">
				a.u_name like CONCAT
				("%",#{find},"%")
				or
				a.u_phone like
				CONCAT
				("%",#{find},"%")
				or
				a.dealers_id like CONCAT
				("%",#{find},"%")
			</if>
		</where>
	</select>
