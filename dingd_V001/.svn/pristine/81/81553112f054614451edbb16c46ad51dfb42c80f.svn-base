	<!-- 查询我的检测单 -->
	<select id="MyDetectOrder" resultType="cn.dingd.dd.entity.CheckBillEntity">
		SELECT
		b. id ,
		b. create_time as createTime ,
		b. allotus_time as
		allotusTime ,
		b. response_time as responseTime ,
		b. check_status as
		checkStatus ,
		b. upload_time as uploadTime ,
		b. check_account as
		checkAccount ,
		b. person ,
		b. telephone ,
		b. create_person as
		create_person ,
		b. address ,
		b. province ,
		b. city ,
		b. area ,
		b. cars ,
		b. car_name as carName ,
		b. car_model as carModel ,
		b. car_type as carType ,
		b.
		car_create_time as carCreateTime ,
		b. car_number as carNumber ,
		b.
		appointment_time as appointmentTime
		FROM check_bill b
		LEFT JOIN
		staff_info s on
		b.staff_id=s.id WHERE
		s.id=#{staffInfo.id} and
		b.check_status=#{checkStatus}
		<!-- limit #{pageObject.startIndex},#{pageObject.pageSize} -->
	</select>
	<!-- /** 我的订单中心修改订单时间 */ -->
	<update id="UpdateTime" parameterType="cn.dingd.dd.entity.CheckBillEntity">
		update check_bill
		set
		appointment_time = #{appointmentTime}
		where id = #{id}
	</update>
	<!-- /** 返回所有的车辆品牌 */ -->
	<select id="CarInfoAll" resultType="cn.dingd.dd.entity.CarBrandsInfoEntity">
		SELECT id,brands_Head as
		brandsHead ,brands_Id as brandsId,brands_Name
		as brandsName,Logo_Imge
		as LogoImge
		from
		car_brands
		<where>
			<if
				test="carBrandsInfoEntity.brandsName!=null and carBrandsInfoEntity.brandsName!=''">
				brands_Name like CONCAT
				("%",#{carBrandsInfoEntity.brandsName},"%")
			</if>
		</where>
	</select>
	<!-- /** 根据品牌ID查询车系列信息 */ -->
	<select id="FindCarSeriesInfo" resultType="cn.dingd.dd.entity.CarSeriesInfoEntity">
		SELECT
		id,
		series_id AS
		seriesId,
		brands_id AS brandsId,
		series_name AS seriesName,
		guide_money
		AS guideMoney,
		series_img AS seriesImg
		FROM car_series WHERE
		brands_id=#{brandsId};
	</select>
	<!-- /** 根据车辆ID查询车型款式信息 */ -->
	<select id="FindCarModelInfo" resultType="cn.dingd.dd.entity.CarModelInfoEntity">
		SELECT
		*
		FROM car_models
		WHERE C3=#{seriesId};
	</select>
	<!-- /** 根据车辆ID获取配置*/ -->
	<select id="FindCarIDInfo" resultType="cn.dingd.dd.entity.CarModelInfoEntity">
		SELECT
		*
		FROM car_models
		WHERE C1=#{C1};
