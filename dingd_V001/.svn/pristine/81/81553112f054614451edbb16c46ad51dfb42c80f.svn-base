<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dingd.dd.dao.DetectOrderDao">
<!-- /**根据检测订单查询是否重复提交信息*/detectUpload -->
	<select id="detectUpload" resultType="int">
	SELECT COUNT(*) FROM t_car_basisinfo WHERE check_bill_id=#{checkBillId}
	</select>
	<!-- /**更改检测单状态*/ setCheckState -->
	<update id="setCheckState">
		update check_bill
		set check_status = #{checkStatus}
		<if test="checkStatus==3">
		,over_time=NOW()
		</if>
		where id = #{id}
	</update>
	<!-- * 上传特写图片和其他证件图片 uploadImg -->
	<insert id="uploadImg" parameterType="java.util.List">
		INSERT INTO car_picture(
		car_id ,
		priture ,
		show_srot ,
		url ,
		cover ,
		create_time ,
		img_type ,
		dd_dict_id
		)
		VALUES
		<foreach collection="list" item="item" index="index"
			separator=",">
			(
			#{item.carId},
			#{item.priture},
			#{item.showSrot},
			#{item.url},
			#{item.cover},
			NOW(),
			#{item.imgType},
			#{item.ddDictId}
			)
		</foreach>
	</insert>
	<!-- 根据检测单号查询对应的检测单id -->
	<select id="getcheckBillId" resultType="cn.dingd.dd.entity.CheckBillEntity">
		SELECT id FROM check_bill
		WHERE check_account=#{checkBillId}
	</select>
	<!-- 根据检测单号查询对应的carId getCarId -->
	<select id="getCarId" resultType="int">
		SELECT id FROM t_car_basisinfo
		WHERE check_bill_id=#{checkBillId}
	</select>
	<!-- 插入损伤信息 -->
	<insert id="UploadCarDamage" parameterType="java.util.List">
		INSERT INTO car_damage(
		car_id,
		damage_type,
		type_number,
		dominant,
		recessive,
		car_describe,
		xy,
		img_url,
		img_remark)
		VALUES
		<foreach collection="list" item="item" index="index"
			separator=",">
			(
			#{item.carId},
			#{item.damageType},
			#{item.typeNumber},
			#{item.dominant},
			#{item.recessive},
			#{item.carDescribe},
			#{item.xy},
			#{item.imgUrl},
			#{item.imgRemark})
		</foreach>
	</insert>
