package cn.dingd.dd.common.entity;

import java.io.Serializable;
import java.util.Date;

/**
* @author 作者 Name:CaoGang
* @version 创建时间：2017年10月10日 下午12:02:37
* 类说明
*/
public class CUserEntity implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = -7224198895178309478L;
	/**C端用户ID*/
	private int id;
	/**C端用户姓名*/
	private String name;
	/**C端用户性别*/
	private int sex;
	/**C端用户年龄*/
	private int age;
	/**C端用户身份证*/
	private String idCard;
	/**C端用户手机号*/
	private String uPhone;
	/**C端用户注册时间*/
	private Date registerTime;
	/**C端用户信誉度*/
	private int credit;
	/**C端用户积分*/
	private int integration;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSex() {
		return sex;
	}
	public void setSex(int sex) {
		this.sex = sex;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getIdCard() {
		return idCard;
	}
	public void setIdCard(String idCard) {
		this.idCard = idCard;
	}
	public String getuPhone() {
		return uPhone;
	}
	public void setuPhone(String uPhone) {
		this.uPhone = uPhone;
	}
	public Date getRegisterTime() {
		return registerTime;
	}
	public void setRegisterTime(Date registerTime) {
		this.registerTime = registerTime;
	}
	public int getCredit() {
		return credit;
	}
	public void setCredit(int credit) {
		this.credit = credit;
	}
	public int getIntegration() {
		return integration;
	}
	public void setIntegration(int integration) {
		this.integration = integration;
	}
	@Override
	public String toString() {
		return "CUserEntity [id=" + id + ", name=" + name + ", sex=" + sex + ", age=" + age + ", idCard=" + idCard
				+ ", uPhone=" + uPhone + ", registerTime=" + registerTime + ", credit=" + credit + ", integration="
				+ integration + "]";
	}

	
	
}
