<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<c:set var="basePath" value="${pageContext.request.contextPath}"></c:set>
<html>
<head>
	<meta charset="UTF-8">
	<title>Login</title>
	<link rel="stylesheet" href="${basePath}/css/test/common.css">
	<link rel="stylesheet" href="${basePath}/css/test/login.css">
	<meta name="viewport" content="width=device-width initial-scale=1 maximum-scale=1, user-scalable=no">
</head>
<body>
	<div class="logo">
		<img src="${basePath}/images/logo.png">
	</div>
	<div id="login">
		<h1>叮当快搜系统登陆</h5>
		<div class="form-group">
			<input type="text" id="user-name" placeholder="请输入账号">
		</div>
		<div class="form-group">
			<input type="password" id="password" placeholder="请输入密码">
		</div>
		<div class="login-btn">登陆</div>
	</div>
	<script src="${basePath}/js/test/jquery-2.1.0.min.js"></script>
	<script src="${basePath}/js/test/login.js"></script>
</body>
</html>